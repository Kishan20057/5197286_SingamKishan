#include <stdio.h>

void miniMaxSum(int arr[], int size) {
    long long total_sum = 0;
    int i, min = arr[0], max = arr[0];

    for(i = 0; i < size; i++) {
        total_sum += arr[i];
        if(arr[i] < min) min = arr[i];
        if(arr[i] > max) max = arr[i];
    }

    long long min_sum = total_sum - max;
    long long max_sum = total_sum - min;

    printf("%lld %lld\n", min_sum, max_sum);
}

int main() {
    int arr[5];
    for(int i = 0; i < 5; i++) {
        scanf("%d", &arr[i]);
    }

    miniMaxSum(arr, 5);
    return 0;
}
